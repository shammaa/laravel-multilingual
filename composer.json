{
    "name": "shammaa/laravel-multilingual",
    "description": "High-performance multilingual package for Laravel with URL localization support and optimized routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shadi Shammaa",
            "email": "shadi.shammaa@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "autoload": {
        "psr-4": {
            "Shammaa\\LaravelMultilingual\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Shammaa\\LaravelMultilingual\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shammaa\\LaravelMultilingual\\LaravelMultilingualServiceProvider"
            ],
            "aliases": {
                "Multilingual": "Shammaa\\LaravelMultilingual\\Facades\\Multilingual"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
